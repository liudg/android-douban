import com.liudong.douban.data.model.movie.MovieList;

CREATE TABLE movie_list (
    category TEXT PRIMARY KEY,
    json TEXT AS MovieList NOT NULL
);

select_by_cate:
SELECT json FROM movie_list WHERE category = ?;

insert_row:
INSERT INTO movie_list(category, json) VALUES(?, ?);

update_json:
UPDATE movie_list SET json = ? WHERE category = ?;